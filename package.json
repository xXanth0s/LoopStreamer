{
  "name": "loopstreamer_ui",
  "version": "0.1.3",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service electron:serve",
    "build": "npm run preload:prod && npm run electron:build",
    "eslint": "eslint . --ext .ts,.vue --ignore-path .eslintignore",
    "eslint:fix": "npm run eslint -- --fix",
    "preload:dev": "webpack --watch --config webpack.dev.js",
    "preload:prod": "webpack --config webpack.prod.js",
    "electron:build": "vue-cli-service electron:build",
    "electron:debug": "vue-cli-service electron:serve --inspect-brk=9223",
    "electron:generate-icons": "electron-icon-builder --input=./other/loopStreamerIcon.png --output=build --flatten",
    "electron:serve": "npm run serve",
    "publish": "npm run preload:prod && npm run electron:build -- -p always",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "remote-dev-tools": "remotedev --hostname=localhost --port=8800"
  },
  "pre-commit": [
    "lint"
  ],
